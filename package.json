{
  "name": "@themuse/bluefin",
  "version": "0.4.2",
  "description": "A postgres schema management tool",
  "main": "index.js",
  "bin": "./bin/cli.js",
  "scripts": {
    "coverage": "./bin/local-coverage",
    "lint:ts": "tslint -c ./tslint.json '*/**/*.{tsx,ts,jsx,js}'",
    "lint:ts:fix": "tslint -c ./tslint.json '*/**/*.{tsx,ts,jsx,js}' --fix",
    "test": "./bin/local-test"
  },
  "author": "Daily Muse, Inc.",
  "license": "MIT",
  "devDependencies": {
    "memfs": "~2.15.2",
    "mocha": "~6.1.4",
    "must": "~0.13.4",
    "nyc": "~14.0.0"
  },
  "dependencies": {
    "@themuse/muse-lint-tslint": "~0.6.1",
    "bluebird": "~3.5.4",
    "commander": "~2.20.0",
    "husky": "~2.1.0",
    "lint-staged": "~8.1.5",
    "pg": "~7.10.0",
    "reify": "~0.18.1"
  },
  "lint-staged": {
    "**/*.{tsx,ts,jsx,js}": [
      "npm run lint:ts --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
