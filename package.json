{
  "name": "@themuse/bluefin",
  "version": "0.2.0",
  "description": "A postgres schema management tool",
  "main": "index.js",
  "bin": "./bin/cli.js",
  "scripts": {
    "coverage": "./bin/local-coverage",
    "lint:ts": "tslint -c ./tslint.json '*.{tsx,ts,jsx,js}' '*/*.{tsx,ts,jsx,js}'",
    "lint:ts:fix": "tslint -c ./tslint.json '*.{tsx,ts,jsx,js}' '*/*.{tsx,ts,jsx,js}' --fix",
    "test": "./bin/local-test"
  },
  "author": "Daily Muse, Inc.",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://verdaccio.inf.themuse.com"
  },
  "devDependencies": {
    "codecov": "~3.1.0",
    "memfs": "~0.0.9",
    "mocha": "~5.2.0",
    "must": "~0.13.4",
    "nyc": "~13.1.0"
  },
  "dependencies": {
    "@themuse/muse-lint-tslint": "~0.3.0",
    "bluebird": "~3.5.3",
    "commander": "~2.19.0",
    "husky": "~1.3.1",
    "lint-staged": "~8.1.5",
    "pg": "~7.8.0",
    "reify": "~0.18.1"
  },
  "lint-staged": {
    "**/*.{tsx,ts,jsx,js}": [
      "npm run lint:ts --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
